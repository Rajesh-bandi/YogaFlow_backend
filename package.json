{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx index.ts",
    "dev:win": "cross-env NODE_ENV=development tsx server/index.ts",
