  "dependencies": {
  "memorystore": "^1.6.7",
  "express": "^4.21.2",
  "cors": "^2.8.5",
  "cross-env": "^10.0.0",
  "dotenv": "^16.4.5",
  "drizzle-kit": "^0.18.1",
  "esbuild": "^0.25.0",
  "tsx": "^4.19.1",
  "typescript": "5.6.3",
  "nodemailer": "^6.10.1",
  "node-cron": "^3.0.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}